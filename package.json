{
  "name": "language-learning-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently -k -n APP,SPEECH -c cyan,yellow \"npm:dev\" \"npm:speech:dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src prisma",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "speech:dev": "node ./scripts/speech-dev.mjs",
    "speech:benchmark": "node ./scripts/benchmark-speech.mjs --dataset scripts/benchmark/smoke-set.json --out docs/benchmark-baseline.md"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.3",
    "jose": "^6.1.3",
    "next": "16.1.6",
    "prisma": "^6.16.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "docx": "^9.5.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
